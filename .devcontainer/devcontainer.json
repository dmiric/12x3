{
    "name": "Medusa.js Dev Container",
    "dockerFile": "Dockerfile",
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [],
    "forwardPorts": [9000], // Change this to the port your Medusa.js backend uses
    "postCreateCommand": "yarn install && yarn start",
    "remoteUser": "codespace",
    "workspaceMount": "source=/workspace,target=/workspace,type=bind,consistency=cached"
  }