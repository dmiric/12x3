{
    "services": {
        "postgres": {
          "image": "postgres:latest",
          "env": {
            "POSTGRES_USER": "your_user",
            "POSTGRES_PASSWORD": "your_password",
            "POSTGRES_DB": "your_database"
          },
          "ports": ["5432:5432"]
        },
        "redis": {
          "image": "redis:latest",
          "ports": ["6379:6379"]
        }
      }
}
